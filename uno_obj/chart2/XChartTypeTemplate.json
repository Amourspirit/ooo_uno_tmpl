    "items": {
      "methods": [
        {
          "name": "applyStyle",
          "returns": "None",
          "desc": [
            "Applies a chart-type specific style (e.g.",
            "",
            "symbols) to all series in the sequence aSeries."
          ],
          "raises": [],
          "args": [
            [
              "xSeries",
              "x_data_series_i",
              "in"
            ],
            [
              "nChartTypeGroupIndex",
              "int",
              "in"
