        "XInteractionHandler"
      ],
      [
        "..embed.x_storage",
        "XStorage"
      ],
      [
        "..beans.property_value",
        "PropertyValue"
      ]
    ],
    "quote": [
      "XStorage",
      "XURI",
      "XMetadatable",
      "typing.List[XURI]",
      "XInputStream",
      "XInteractionHandler",
      "typing.List[PropertyValue]",
